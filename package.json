{
  "name": "sunseekerx-doc",
  "version": "1.0.0",
  "private": true,
  "description": "sunseekerx's notebook v1.",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider vuepress build docs",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider vuepress dev docs",
    "gc": "git add -A && git-cz && git pull && git push",
    "postinstall": "husky install",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write --loglevel warn \"./**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "pkg:sort": "npx sort-package-json",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@vuepress/plugin-search": "^1.9.10",
    "cross-env": "^7.0.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "pinst": "^3.0.0",
    "prettier": "^3.3.3",
    "vuepress": "^1.9.10",
    "vuepress-theme-hope": "1.30.0"
  },
  "packageManager": "yarn@4.3.1"
}

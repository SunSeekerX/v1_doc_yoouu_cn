{
  "name": "sunseekerx-doc",
  "version": "1.0.0",
  "private": true,
  "description": "sunseekerx's notebook v1.",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider vuepress build docs",
    "dev": "vuepress dev docs",
    "gc": "git add -A && git-cz && git pull && git push",
    "lint:prettier": "prettier --write --loglevel warn \"./**/*.{js,json,tsx,css,less,scss,vue,html,md}\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@vuepress/plugin-search": "^1.9.10",
    "cross-env": "^7.0.3",
    "pinst": "^3.0.0",
    "prettier": "^3.3.1",
    "vuepress": "^1.9.10",
    "vuepress-theme-hope": "1.30.0"
  },
  "packageManager": "yarn@4.2.2"
}
